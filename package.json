{
  "name": "ap-widget-repo",
  "version": "1.0.29",
  "description": "Project to build SICK Analytics Platform widgets",
  "main": "./dist",
  "scripts": {
    "babel": "babel ./src --out-dir ./dist",
    "build": "cross-env NODE_ENV=production npm run babel && npm run sass",
    "bundle": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.js --progress",
    "bundle:dev": "cross-env NODE_ENV=development webpack --config ./build/webpack.config.development.js --progress",
    "clean": "rimraf ./dist",
    "docs": "rimraf ./docs && esdoc -c .esdocrc",
    "lint": "eslint ./ --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "prebuild": "npm run clean && npm run lint",
    "sass": "node-sass --include-path ./src ./src/style.scss ./dist/style.css",
    "start": "node ./server",
    "test": "karma start build/karma.conf.js",
    "test:watch": "karma start build/karma.conf.js --no-single-run"
  },
  "keywords": [
    "widget",
    "repository",
    "SICK",
    "Analytics",
    "Platform"
  ],
  "author": "SICK",
  "license": "UNLICENSED",
  "private": false,
  "repository": {
    "type": "svn",
    "url": ""
  },
